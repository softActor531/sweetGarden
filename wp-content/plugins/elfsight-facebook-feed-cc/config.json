            },
            {
              "value": "ru",
              "name": "Русский"
            },
            {
              "value": "pt",
              "name": "Português"
            },
            {
              "value": "sv",
              "name": "Svenska"
            },
            {
              "value": "pl",
              "name": "Polski"
            },
            {
              "value": "no",
              "name": "Norsk"
            },
            {
              "value": "it",
              "name": "Italiano"
            },
            {
              "value": "hu",
              "name": "Magyar"
            },
            {
              "value": "fi",
              "name": "Finnish"
            },
            {
              "value": "dk",
              "name": "Dansk"
            },
            {
              "value": "tr",
              "name": "Turkish"
            },
            {
              "value": "ar",
